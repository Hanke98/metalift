#include <stdlib.h>
#include <assert.h>

#define MAXLEN 10

struct list
{
  int vals [MAXLEN];
  int len;
};

typedef struct list *List;

List newList() 
{
  List r = (List)malloc(sizeof(struct list));
  r->len = 0;
  return r;
}

int listLength (List l) { return l->len; }

List listAppend (List in, int e) 
{ 
  assert(in->len < MAXLEN);
  List r = newList();
  for (int i = 0; i < in->len; ++i) 
    r->vals[i] = in->vals[i]; 

  r->vals[in->len] = e;
  r->len = in->len + 1;
   
  return r; 
}

int listGet (List l, int i) 
{ 
  assert(i < l->len);
  return l->vals[i];
}

